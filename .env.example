# Proof of Data Possession (PDP) Service Configuration
PDP_API_KEY=your_pdp_api_key_here
PDP_BASE_URL=https://pdp-server.example.com

# Bot tokens (at least one is required)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Default Model Configuration (fallback for all modules)
MODEL_TYPE=openai  # Options: openai, anthropic, google, secretai

# Module-specific Model Configuration (optional, falls back to MODEL_TYPE if not set)
CHAT_MODEL_TYPE=secretai
EMBEDDING_MODEL_TYPE=openai
SUMMARIZE_MODEL_TYPE=openai

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key
ANTHROPIC_BASE_URL=https://api.anthropic.com
ANTHROPIC_MODEL=claude-2

# Google Configuration
GOOGLE_API_KEY=your_google_api_key
GOOGLE_MODEL=gemini-pro

# SecretAI Configuration
SECRETAI_API_KEY=your_secretai_api_key
SECRETAI_BASE_URL=https://ai1.scrtlabs.com:21434
SECRETAI_MODEL=deepseek-r1:70b

# Chroma configuration
CHROMA_URL=http://localhost:8000
